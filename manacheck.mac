#define manaINI hastecheck.ini

Sub Main
	/call WriteMana
/return

Sub WriteMana
    /echo writingMana:
    /declare df  string local ${Me.CleanName}
    /ini "manaINI" "Mana" "${Me.CleanName}" "${Me.CurrentMana}"
/return 








